{
    "buildCommand": "npm run docs:build",
    "outputDirectory": ".vitepress/dist",
    "installCommand": "npm install",
    "framework": "vitepress",
    "cleanUrls": true,
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000, immutable"
                }
            ]
        }
    ]
}
